{
    "name": "javaabu/boli-api",
    "description": "PHP SDK for Ministry of Economic Development's Boli System",
    "homepage": "https://github.com/Javaabu/boli-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Jailam"
            "email": "jailam@javaabu.com"
        },
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "guzzlehttp/guzzle": "~7.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\Boli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\Boli\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Javaabu\\Boli\\BoliServiceProvider"
            ],
            "aliases": {
                "Boli": "Javaabu\\Boli\\Facades\\Boli"
            }
        }
    },
    "minimum-stability": "stable"
}
